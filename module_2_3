my_list = [42, 69, 0, 322, 13, 0, 99, -5, 9, 8, 7, -6, 5]
a = 0
while a < len(my_list):
        if my_list[a] >= 0: #Проверяем положительные числа
            b = my_list[a]
            a = a + 1
            if b > 0: # Проверяем положительные числа, отличные от нуля
                print(b)
                continue
        else:
            break
print('Finish')
