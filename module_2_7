def print_params(a=1, b='строка', c=True):
    print(a, b, c)
    
print_params()
print_params(a = True, c = [22, 2, 22])
print_params(b = 25)
print_params(c = [1, 2, 3])
values_list = ['list', 15, True]
values_dict = {'a': 7, 'b': True, 'c': 'str'}
print_params(*values_list)
print_params(**values_dict)
values_list_2 = ['list2', False]
print_params(*values_list_2, 42)
